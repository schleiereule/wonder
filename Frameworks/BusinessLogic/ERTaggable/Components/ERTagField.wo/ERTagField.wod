ID : WOString {
  value = id;
}

TagsID : WOString {
  value = tagsID;
}

JavascriptTags : WOString {
  value = javascriptAvailableTags;
}

TagField : WOTextField {
  value = tags;
  size = size;
  id = id;
  style = ^style;
  class = ^class;
}

IsCaptionUI : WOConditional {
  condition = isCaptionUIStyle;
}

IsPopupUI : WOConditional {
	condition = isCaptionUIStyle;
	negate = true;
}

AvailableTags : WOGenericContainer {
  elementName = "div";
  id = tagsID;
  class = "availableTags";
}
